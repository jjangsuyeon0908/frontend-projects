let regex = /(\d{3})-(\d{4})-(\d{4})/;
let phone = "010-1234-5678";

let match = phone.match(regex);
if(match) {
  let areaCode = match[1];
  let mainNumber = `${match[2]}-${match[3]}`;
  console.log(`ì§€ì—­ ì½”ë“œ : ${areaCode}`);
  console.log(`ë²ˆí˜¸ : ${mainNumber}`);
